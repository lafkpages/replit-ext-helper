{
  "name": "replit-ext-helper",
  "version": "0.0.6",
  "author": "LuisAFK",
  "main": "dist/index.js",
  "types": "dist/types.d.ts",
  "files": [
    "dist",
    "!dist/userscript.js"
  ],
  "scripts": {
    "build": "bun scripts/build.js",
    "build:userscript": "mkdir -p dist && rm -f dist/userscript.js && cp src/userscript.js dist/userscript.js && bun build src/index.js --minify >> dist/userscript.js",
    "prepublishOnly": "bun run build"
  },
  "dependencies": {
    "@replit-svelte/ui": "^1.4.7",
    "svelte": "^4.2.1"
  },
  "devDependencies": {
    "@replit-svelte/types": "^1.4.1",
    "bun-types": "^1.0.2",
    "esm-env": "^1.0.0"
  }
}
